{
  "compilerOptions": {
    "allowJs": false,
    "declaration": false,
    "inlineSourceMap": false,
    "lib": ["es5", "es6", "es2015.collection", "es2015.iterable", "es2015.promise", "dom"],
    "moduleResolution": "node",
    "newLine": "lf",
    "noImplicitAny": true,
    "noImplicitReturns": true,
    "noUnusedLocals": true,
    "noUnusedParameters": true,
    "pretty": true,
    "removeComments": false,
    "strictNullChecks": true,
    "types": ["node", "jasmine"]
  }
}
